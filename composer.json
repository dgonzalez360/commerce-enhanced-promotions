{
    "name": "webdna/commerce-advanced-promotions",
    "description": "Advanced commerce promotions",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "info@webdna.co.uk",
        "issues": "https://github.com/webdna/commerce-advanced-promotions/issues?state=open",
        "source": "https://github.com/webdna/commerce-advanced-promotions",
        "docs": "https://github.com/webdna/commerce-advanced-promotions",
        "rss": "https://github.com/webdna/commerce-advanced-promotions/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.3.5"
    },
    "autoload": {
        "psr-4": {
            "webdna\\commerce\\advancedpromotions\\": "src/"
        }
    },
    "extra": {
        "handle": "commerce-advanced-promotions",
        "name": "Advanced Promotions",
        "developer": "webdna",
        "documentationUrl": "https://github.com/webdna/commerce-advanced-promotions",
        "class": "webdna\\commerce\\advancedpromotions\\AdvancedPromotions"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

